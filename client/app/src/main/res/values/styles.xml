<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppLogo">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/ic_natour_logo</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.CornerSize50Percent" parent="">
        <item name="cornerSize">50%</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.CornerRoundSquarePercent" parent="Widget.AppCompat.ActionButton">
        <item name="cornerSize">5%</item>
    </style>

    <style name="ThemeOverlay.NaTour.ElevationOverlayDisabled" parent="">
        <item name="elevationOverlayEnabled">false</item>
    </style>
    
    <style name="Theme.Natour.Widget.ActionButton" parent="Widget.AppCompat.ActionButton">
        <item name="textAllCaps">false</item>
        <item name="android:textAppearance">?attr/textAppearanceButton</item>
    </style>

</resources>